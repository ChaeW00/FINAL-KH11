<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="freeChatRoom">

 	<select id="find" resultType="FreeChatRoomDto">
 		select * from free_chat_room where room_name = #{roomName}
 	</select>
 	<insert id="insert">
 		insert into free_chat_room values(#{roomName})
 	</insert>
 </mapper>